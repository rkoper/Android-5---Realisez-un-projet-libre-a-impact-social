<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/grey_50"
    android:orientation="horizontal">


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/home_gl_h_97"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.98" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/home_gl_h_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.92" />

        <ImageButton
            android:elevation="1dp"
            android:id="@+id/ic_facebook"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="@android:dimen/notification_large_icon_width"
            android:layout_marginLeft="@android:dimen/notification_large_icon_width"
            android:layout_marginTop="32dp"
            android:background="@drawable/ic_facebook"
            app:layout_constraintBottom_toBottomOf="@+id/home_gl_h_9"
            app:layout_constraintEnd_toStartOf="@+id/ic_twitter"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:elevation="5dp"
            android:id="@+id/ic_twitter"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/ic_twitter"
            app:layout_constraintBottom_toBottomOf="@+id/home_gl_h_9"
            app:layout_constraintEnd_toStartOf="@+id/ic_linkedin"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ic_facebook"/>



        <ImageButton
            android:elevation="9dp"
            android:id="@+id/ic_linkedin"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/ic_linkedin"
            app:layout_constraintBottom_toBottomOf="@+id/ic_twitter"
            app:layout_constraintEnd_toStartOf="@+id/ic_youtube"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ic_twitter"
            app:layout_constraintTop_toTopOf="@+id/ic_twitter" />

        <ImageButton
            android:elevation="14dp"
            android:id="@+id/ic_youtube"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/ic_youtube"
            app:layout_constraintBottom_toBottomOf="@+id/ic_linkedin"
            app:layout_constraintEnd_toStartOf="@+id/ic_sms"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ic_linkedin"
            app:layout_constraintTop_toTopOf="@+id/ic_linkedin" />

        <ImageButton
            android:elevation="19dp"
            android:id="@+id/ic_sms"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/ic_sms"
            android:backgroundTint="@color/black"
            app:layout_constraintBottom_toBottomOf="@+id/ic_youtube"
            app:layout_constraintEnd_toStartOf="@+id/ic_mail"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ic_youtube"
            app:layout_constraintTop_toTopOf="@+id/ic_youtube" />

        <ImageButton
            android:elevation="24dp"
            android:id="@+id/ic_mail"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginEnd="@android:dimen/notification_large_icon_width"
            android:layout_marginRight="@android:dimen/notification_large_icon_width"
            android:background="@drawable/ic_envelope"
            app:layout_constraintBottom_toBottomOf="@+id/ic_sms"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ic_sms"
            app:layout_constraintTop_toTopOf="@+id/ic_sms" />


        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BringDing 1.0"
            android:gravity="center"
            android:textSize="12sp"
            style="@style/TxtViewPrincipalBlack"
            app:layout_constraintBottom_toBottomOf="@+id/home_gl_h_97"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                        <androidx.cardview.widget.CardView
                            android:id="@+id/card_t_top_profil"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginLeft="16dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="16dp"
                            android:layout_marginRight="16dp"
                            android:elevation="0dp"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="2dp"
                            app:cardMaxElevation="6dp"
                            app:layout_constraintBottom_toTopOf="@+id/guideline"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/guideline2">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:background="@color/grey_50"
                                    android:elevation="3dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                        <androidx.constraintlayout.widget.Guideline
                                            android:id="@+id/guideline11"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal"
                                            app:layout_constraintGuide_percent="0.5" />
                                        <ImageView
                                            android:background="@drawable/round_black"
                                            android:backgroundTint="@color/black"
                                            app:layout_constraintTop_toBottomOf="@+id/guideline11"
                                            app:layout_constraintBottom_toTopOf="@+id/guideline11"
                                            app:layout_constraintStart_toStartOf="parent"
                                            app:layout_constraintEnd_toStartOf="@+id/card_t_profil_name"
                                            android:layout_gravity="left|center_vertical"
                                            android:id="@+id/card_t_profil_photo"
                                            android:layout_width="75dp"
                                            android:layout_height="75dp"
                                            android:scaleType="fitXY"
                                            tools:ignore="RtlHardcoded" />
                                        <TextView
                                            android:text="Nom PrÃ©non"
                                            style="@style/TxtViewPrincipalBlack"
                                            app:layout_constraintTop_toTopOf="parent"
                                            app:layout_constraintEnd_toEndOf="parent"
                                            app:layout_constraintBottom_toTopOf="@+id/guideline11"
                                            app:layout_constraintStart_toEndOf="@+id/card_t_profil_photo"
                                            android:id="@+id/card_t_profil_name"
                                            android:layout_width="225dp"
                                            android:layout_height="40dp"
                                            android:layout_marginTop="8dp"
                                            android:layout_gravity="center"
                                            android:textAlignment="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Large" />
                                        <TextView
                                            app:layout_constraintEnd_toEndOf="parent"
                                            style="@style/TxtViewPrincipalBlack"
                                            app:layout_constraintTop_toBottomOf="@+id/guideline11"
                                            app:layout_constraintStart_toEndOf="@+id/card_t_profil_photo"
                                            android:id="@+id/card_t_profil_status"
                                            android:layout_width="wrap_content"
                                            android:layout_margin="2dp"
                                            android:padding="2dp"
                                            android:layout_height="25dp"
                                            android:layout_marginTop="8dp"
                                            android:text="Newbie"
                                            android:textAlignment="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                            android:layout_gravity="center|bottom" />
                                        <ImageView
                                            style="@style/TxtViewPrincipalBlack"
                                            app:layout_constraintTop_toTopOf="@+id/card_t_profil_status"
                                            app:layout_constraintBottom_toBottomOf="@+id/card_t_profil_status"
                                            app:layout_constraintStart_toEndOf="@+id/card_t_profil_status"
                                            android:id="@+id/img3"
                                            android:background="@drawable/medalone"
                                            android:layout_width="30dp"
                                            android:layout_height="30dp"
                                            android:text="BringDring"
                                            android:textAlignment="center"
                                            android:textAppearance="@style/TextAppearance.AppCompat.Large"
                                            android:layout_gravity="center|bottom" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <ImageView
                            app:layout_constraintTop_toBottomOf="@+id/card_t_top_profil"
                            app:layout_constraintBottom_toTopOf="@+id/guideline25"
                            app:layout_constraintEnd_toEndOf="@+id/guideline_v_5"
                            android:id="@+id/imageView"
                            android:layout_width="120dp"
                            android:layout_height="30dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/dblogo" />

                        <TextView
                            app:layout_constraintStart_toStartOf="@+id/guideline_v_5"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/card_t_top_profil"
                            app:layout_constraintBottom_toTopOf="@+id/guideline25"
                            android:id="@+id/textView3"
                            style="@style/TxtViewPrincipalBlack"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="Improve your party !"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline25"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.35" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline_v_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintGuide_percent="0.5" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.25" />
                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.37" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.03" />

                        <androidx.constraintlayout.widget.Guideline
                            android:id="@+id/guideline81"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            app:layout_constraintGuide_percent="0.85" />



                        <androidx.cardview.widget.CardView
                            android:id="@+id/card_t_setting"
                            android:layout_width="120dp"
                            android:layout_height="135dp"
                            android:layout_marginEnd="50dp"
                            android:layout_marginRight="50dp"
                            app:cardCornerRadius="5dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/card_t_profil"
                            app:layout_constraintTop_toTopOf="@+id/card_t_profil">

                                <LinearLayout
                                    android:background="@drawable/rectangle_corner_white"
                                    android:backgroundTint="@color/green_300"
                                    android:id="@+id/card_t_settings_click"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:orientation="vertical"
                                    >

                                        <ImageView
                                            android:padding="10dp"
                                            android:layout_weight="3"
                                            android:id="@+id/img__speakers"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:src="@drawable/settings"
                                            app:tint="@color/white"/>

                                        <TextView
                                            style="@style/TxtViewPrincipalWhite"
                                            android:textStyle="bold"
                                            android:textAlignment="textEnd"
                                            android:id="@+id/text__speakers"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Settings"
                                            android:textSize="16sp" />
                                </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card_t_profil"
                            android:layout_width="120dp"
                            android:layout_height="135dp"
                            android:layout_marginStart="50dp"
                            android:layout_marginLeft="50dp"
                            android:layout_marginTop="24dp"
                            app:cardCornerRadius="5dp"
                            app:layout_constraintEnd_toStartOf="@+id/card_t_setting"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/card_t_create">

                                <LinearLayout
                                    android:background="@drawable/rectangle_corner_white"
                                    android:backgroundTint="@color/purple_300"
                                    android:id="@+id/card_t_profil_click"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:orientation="vertical"
                                    >

                                        <ImageView
                                            android:padding="10dp"
                                            android:layout_weight="3"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:src="@drawable/personn"
                                            app:tint="@color/white"/>

                                        <TextView
                                            style="@style/TxtViewPrincipalWhite"
                                            android:textStyle="bold"
                                            android:textAlignment="textEnd"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Profil"
                                            android:textSize="16sp" />
                                </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_margin="5dp"
                            android:id="@+id/card_t_event"
                            android:layout_width="120dp"
                            android:layout_height="135dp"
                            android:layout_marginStart="50dp"
                            android:layout_marginLeft="50dp"
                            android:layout_marginTop="24dp"
                            app:cardCornerRadius="5dp"
                            app:layout_constraintEnd_toStartOf="@+id/card_t_create"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/guideline5">

                                <LinearLayout
                                    android:background="@drawable/rectangle_corner_white"
                                    android:backgroundTint="@color/yellow_700"
                                    android:id="@+id/card_t_event_click"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:orientation="vertical">

                                        <ImageView
                                            android:padding="10dp"
                                            android:layout_weight="3"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:src="@drawable/star"
                                            app:tint="@color/white"/>

                                        <TextView
                                            style="@style/TxtViewPrincipalWhite"
                                            android:textStyle="bold"
                                            android:textAlignment="textEnd"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Event"
                                            android:textSize="20sp" />
                                </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_margin="5dp"
                            android:id="@+id/card_t_create"
                            android:layout_width="120dp"
                            android:layout_height="135dp"
                            app:cardCornerRadius="5dp"
                            app:layout_constraintEnd_toStartOf="@+id/card_t_calendar"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/card_t_event"
                            app:layout_constraintTop_toTopOf="@+id/guideline5">

                                <LinearLayout
                                    android:background="@drawable/rectangle_corner_white"
                                    android:backgroundTint="@color/blue_300"
                                    android:id="@+id/card_t_create_click"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:orientation="vertical"
                                    >

                                        <ImageView
                                            android:padding="10dp"
                                            android:layout_weight="3"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:src="@drawable/addcircle"
                                            app:tint="@color/white"/>

                                        <TextView
                                            style="@style/TxtViewPrincipalWhite"
                                            android:textStyle="bold"
                                            android:textAlignment="textEnd"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Create"
                                            android:textSize="16sp" />
                                </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_margin="5dp"
                            android:id="@+id/card_t_calendar"
                            android:layout_width="120dp"
                            android:layout_height="135dp"
                            android:layout_marginEnd="50dp"
                            android:layout_marginRight="50dp"
                            app:cardCornerRadius="5dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/card_t_create"
                            app:layout_constraintTop_toTopOf="@+id/guideline5">

                                <LinearLayout
                                    android:background="@drawable/rectangle_corner_white"
                                    android:backgroundTint="@color/red_300"
                                    android:id="@+id/card_t_calendar_click"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:orientation="vertical"
                                    >
                                        <ImageView
                                            android:padding="10dp"
                                            android:layout_weight="3"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:src="@drawable/calendar"
                                            app:tint="@color/white"/>

                                        <TextView
                                            style="@style/TxtViewPrincipalWhite"
                                            android:textStyle="bold"
                                            android:textAlignment="textEnd"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Calendar"
                                            android:textSize="16sp" />
                                </LinearLayout>
                        </androidx.cardview.widget.CardView>

                </androidx.constraintlayout.widget.ConstraintLayout>






</androidx.constraintlayout.widget.ConstraintLayout>

