 <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorTransparent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="350dp"
        android:layout_height="550dp"
        android:background="@drawable/rectangle_corner_grey_600"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_h_4"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_h_6"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_h_8"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.54"/>
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/dialog_det_info_h_5"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5"/>
            <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/dialog_det_info_h_81"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.9"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_h_9"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.99"/>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_v_1"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.01"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_v_8"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.99"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_det_info_v_9"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.97"/>
            <com.google.android.material.textfield.TextInputLayout
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/detail_d__name_txt"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:boxStrokeColor="@color/white"
                app:layout_constraintRight_toRightOf="@+id/dialog_det_info_v_9"
                app:layout_constraintLeft_toLeftOf="@+id/dialog_det_info_v_1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_4"
                android:cursorVisible="false"
                android:hint="@string/event_name"
                style="@style/LoginTextInputLayoutStyleV3"
                android:textColorHint="@color/white">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/detail_d__name_edit"
                    style="@style/TxtViewPrincipalWhite"
                    app:boxStrokeColor="@color/white"
                    android:gravity="start"
                    android:cursorVisible="false"
                    android:text=" "
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </com.google.android.material.textfield.TextInputLayout>
            <TextView
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="10dp"
                android:textSize="22sp"
                android:textColor="@color/white"
                android:text="12345"
                style="@style/TxtViewPrincipalWhitev3"
                android:id="@+id/dialog_det_info_nameevent"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintRight_toRightOf="@+id/dialog_det_info_v_9"
                app:layout_constraintLeft_toLeftOf="@+id/dialog_det_info_v_1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_4"/>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/_dialog_detail_date"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:boxStrokeColor="@color/white"
                app:layout_constraintRight_toLeftOf="@+id/dialog_det_info_h_5"
                app:layout_constraintLeft_toLeftOf="@+id/dialog_det_info_v_1"
                app:layout_constraintTop_toTopOf="@id/dialog_det_info_h_4"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_6"
                android:cursorVisible="false"
                android:hint="@string/date"
                style="@style/LoginTextInputLayoutStyleV3"
                android:textColorHint="@color/white">

                <com.google.android.material.textfield.TextInputEditText
                    style="@style/TxtViewPrincipalWhite"
                    app:boxStrokeColor="@color/white"
                    android:gravity="start"
                    android:cursorVisible="false"
                    android:text=" "
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </com.google.android.material.textfield.TextInputLayout>
        <TextView
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="5dp"
            android:padding="1dp"
            android:textSize="16sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhitev3"
            android:id="@+id/dialog_det_info_dateevent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintRight_toRightOf="@+id/_dialog_detail_date"
            app:layout_constraintLeft_toLeftOf="@+id/_dialog_detail_date"
            app:layout_constraintTop_toTopOf="@id/_dialog_detail_date"
            app:layout_constraintBottom_toBottomOf="@+id/_dialog_detail_date"/>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/dialog_detail_hour"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:boxStrokeColor="@color/white"
                app:layout_constraintLeft_toRightOf="@+id/dialog_det_info_h_5"
                app:layout_constraintRight_toRightOf="@+id/dialog_det_info_v_9"
                app:layout_constraintTop_toTopOf="@id/dialog_det_info_h_4"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_6"
                android:cursorVisible="false"
                android:hint="@string/hour"
                style="@style/LoginTextInputLayoutStyleV3"
                android:textColorHint="@color/white">

                <com.google.android.material.textfield.TextInputEditText
                    style="@style/TxtViewPrincipalWhite"
                    app:boxStrokeColor="@color/white"
                    android:gravity="start"
                    android:cursorVisible="false"
                    android:text=" "
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </com.google.android.material.textfield.TextInputLayout>
        <TextView
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="5dp"
            android:padding="1dp"
            android:textSize="16sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhitev3"
            android:id="@+id/dialog_det_info_hourevent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_detail_hour"
            app:layout_constraintRight_toRightOf="@+id/dialog_detail_hour"
            app:layout_constraintTop_toTopOf="@id/dialog_detail_hour"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_detail_hour"/>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/_dialog_detail_name"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:boxStrokeColor="@color/white"
                app:layout_constraintLeft_toLeftOf="@+id/dialog_det_info_v_1"
                app:layout_constraintRight_toRightOf="@+id/dialog_det_info_v_9"
                app:layout_constraintTop_toTopOf="@id/dialog_det_info_h_6"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_8"
                android:cursorVisible="false"
                android:hint="@string/address"
                style="@style/LoginTextInputLayoutStyleV3"
                android:textColorHint="@color/white">

                <com.google.android.material.textfield.TextInputEditText
                    style="@style/TxtViewPrincipalWhite"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:cursorVisible="false"
                    android:gravity="start"
                    android:text=" "
                    app:boxStrokeColor="@color/white" />
            </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="5dp"
            android:padding="1dp"
            android:textSize="16sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhitev3"
            android:ellipsize="end"
            android:id="@+id/dialog_det_info_addressevent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/_dialog_detail_name"
            app:layout_constraintRight_toRightOf="@+id/_dialog_detail_name"
            app:layout_constraintTop_toTopOf="@id/_dialog_detail_name"
            app:layout_constraintBottom_toBottomOf="@+id/_dialog_detail_name"/>


            <com.google.android.material.textfield.TextInputLayout
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/_dialog_detail_desc"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:boxStrokeColor="@color/white"
                app:layout_constraintLeft_toLeftOf="@+id/dialog_det_info_v_1"
                app:layout_constraintRight_toRightOf="@+id/dialog_det_info_v_9"
                app:layout_constraintTop_toTopOf="@id/dialog_det_info_h_8"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_81"
                android:cursorVisible="false"
                android:hint="@string/description"
                style="@style/LoginTextInputLayoutStyleV3"
                android:textColorHint="@color/white">

                <com.google.android.material.textfield.TextInputEditText
                    style="@style/TxtViewPrincipalWhite"
                    app:boxStrokeColor="@color/white"
                    android:gravity="start"
                    android:cursorVisible="false"
                    android:text=" "
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </com.google.android.material.textfield.TextInputLayout>
            <TextView
                android:layout_marginTop="25dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:padding="1dp"
                android:textSize="16sp"
                android:textColor="@color/white"
                android:id="@+id/dialog_det_info_descevent"
                android:fontFamily="@font/roboto"
                android:ellipsize="end"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="@+id/_dialog_detail_desc"
                app:layout_constraintRight_toRightOf="@+id/_dialog_detail_desc"
                app:layout_constraintTop_toTopOf="@id/_dialog_detail_desc"
                app:layout_constraintBottom_toBottomOf="@+id/_dialog_detail_desc"/>


            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/dialog_det_info_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/grey_600"
                android:rotation="45"
                app:backgroundTint="@color/red_300"
                app:borderWidth="2dp"
                app:fabCustomSize="35dp"
                app:layout_constraintBottom_toBottomOf="@+id/dialog_det_info_h_9"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/dialog_det_info_h_81"
                app:maxImageSize="22dp"
                app:srcCompat="@drawable/logo_add"
                app:tint="@android:color/white" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>