<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    style="@style/MyDialogTheme"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorTransparent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="250sp"
        android:layout_height="200sp"
        android:background="@drawable/rectangle_corner_grey_600"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="220dp"
            android:layout_height="180sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent">
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_1"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.08"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_2"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.27"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_4"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.44"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_6"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_8"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_h_9"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95"/>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_v_1"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_v_8"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.85"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dialog_cal_v_9"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9"/>

        <ImageView
            android:layout_margin="5dp"
            android:id="@+id/dialog_cal_photouser"
            android:text="Name"
            android:background="@drawable/round_small"
            android:layout_width="35dp"
            android:layout_height="35dp"
            app:layout_constraintRight_toRightOf="@+id/dialog_cal_v_8"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_1"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_2" />

        <TextView
            android:textColor="@color/white"
            android:textSize="14sp"
            style="@style/TxtViewPrincipalWhite"
            android:id="@+id/dialog_cal_nameuser"
            android:text="Name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toLeftOf="@+id/dialog_cal_photouser"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_cal_v_1"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_1"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_2"/>
        <TextView
            android:padding="1dp"
            android:textSize="22sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhite"
            android:id="@+id/dialog_cal_nameevent"
            android:text="Name Party"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@+id/dialog_cal_v_9"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_cal_v_1"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_2"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_4"/>
        <TextView
            android:padding="1dp"
            android:textSize="15sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhite"
            android:id="@+id/dialog_cal_dateevent"
            android:text="Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toLeftOf="@+id/dialog_cal_hourevent"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_cal_v_1"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_4"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_6"/>
        <TextView
            android:padding="1dp"
            android:textSize="15sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhite"
            android:id="@+id/dialog_cal_hourevent"
            android:text="Hour"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/dialog_cal_dateevent"
            app:layout_constraintRight_toRightOf="@+id/dialog_cal_v_9"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_4"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_6"/>
        <TextView

            android:padding="1dp"
            android:textSize="20sp"
            android:textColor="@color/white"
            style="@style/TxtViewPrincipalWhite"
            android:text="Addrddddddddddddddddddddddddddess"
            android:id="@+id/dialog_cal_addressevent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_cal_v_1"
            app:layout_constraintRight_toRightOf="@+id/dialog_cal_v_9"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_6"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_8"/>


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/dialog_cal_cancel"
            app:layout_constraintLeft_toLeftOf="@+id/dialog_cal_v_1"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_8"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_9"
            app:layout_constraintRight_toLeftOf="@+id/dialog_cal_see"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fabCustomSize="35dp"
            app:borderWidth="2dp"
            app:maxImageSize="22dp"
            app:srcCompat="@drawable/logo_add"
            android:rotation="45"
            app:tint="@android:color/white"
            android:backgroundTint="@color/grey_600"
            app:backgroundTint="@color/green_300" />


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/dialog_cal_see"
            app:layout_constraintLeft_toRightOf="@+id/dialog_cal_cancel"
            app:layout_constraintRight_toRightOf="@+id/dialog_cal_v_9"
            app:layout_constraintTop_toTopOf="@id/dialog_cal_h_8"
            app:layout_constraintBottom_toBottomOf="@+id/dialog_cal_h_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:fabCustomSize="35dp"
            app:borderWidth="2dp"
            app:maxImageSize="22dp"
            app:srcCompat="@drawable/logo_eye"
            app:tint="@android:color/white"
            android:backgroundTint="@color/grey_600"
            app:backgroundTint="@color/green_300" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>